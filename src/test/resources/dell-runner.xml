<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Automation smoke Test">
    <parameter name="useCloudEnv" value="False"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="11"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="browserVersion" value="102"></parameter>
    <parameter name="url" value="https://www.dell.com/en-us"></parameter>

    <test name="Search Test">
        <classes>
            <class name="Application.dell.SearchTest"></class>
        </classes>
    </test>

    <test name="SignIn Test">
        <classes>
            <class name="Application.dell.SignInTest"></class>
        </classes>
    </test>

    <test name="DropDown Test">
        <classes>
            <class name="Application.dell.HeaderMenuOptionTest"></class>
        </classes>
    </test>

    <test name="Footer Option Test">
        <classes>
            <class name="Application.dell.FooterOptionsTest"></class>
        </classes>
    </test>

    <test name="RadioBtn Test">
        <classes>
            <class name="Application.dell.RadioButtonTest"></class>
        </classes>
    </test>

    <test name="Homepage Menu Test">
        <classes>
            <class name="Application.dell.HomepageMenuOptionTest"></class>
        </classes>
    </test>

    <test name="Scroll Bar Test">
        <classes>
            <class name="Application.dell.ScrollBarTest"></class>
        </classes>
    </test>

    <test name="Cart Test">
        <classes>
            <class name="Application.dell.CartTest"></class>
        </classes>
    </test>

    <test name="CheckBox Test">
        <classes>
            <class name="Application.dell.CheckBoxTest"></class>
        </classes>
    </test>




</suite>